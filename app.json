{
  "expo": {
    "name": "rn-starter-1",
    "slug": "rn-starter-1",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "upload images from gallery.",
          "cameraPermission": "upload images by camera."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow app to access current location?"
      }
    },
    "android": {
      "adaptiveIcon": {
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
