{
  "expo": {
    "name": "JFinsurance",
    "slug": "JFinsurance",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "upload images from gallery.",
          "cameraPermission": "upload images by camera."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "upload images by camera",
        "NSLocationWhenInUseUsageDescription": "Allow app to access current location?"
      },
      "bundleIdentifier": "Michael.jfinsurance"
    },
    "android": {
      "adaptiveIcon": {},
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e936360e-2d9f-460a-8870-150bc7ab4e54"
      }
    }
  }
}
